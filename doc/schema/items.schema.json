{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Item",
  "type": "object",
  "required": ["itemId", "slot", "resolved", "createdAt"],
  "properties": {
    "itemId": { "type": "string" },
    "owner": { "type": ["string", "null"] },
    "slot": { "type": "string" },
    "baseTemplate": { "type": "string" },
    "resolved": { "type": "object" },
    "createdAt": { "type": "string", "format": "date-time" },
    "seed": { "type": "integer" }
  }
}
